{
  "name": "biedrapp",
  "version": "0.0.0",
  "description": "This is a **monorepo** containing both the frontend and backend code for the Biedrapp project.",
  "homepage": "https://github.com/culplate/biedrapp#readme",
  "bugs": {
    "url": "https://github.com/culplate/biedrapp/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/culplate/biedrapp.git"
  },
  "license": "UNLICENSED",
  "author": "V&K",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "install:all": "npm run install:frontend && npm run install:backend",
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && npm install",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "cd frontend && npm test",
    "test:backend": "cd backend && npm test"
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
